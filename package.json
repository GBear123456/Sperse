  "name": "abp-zero-template",
  "version": "8.6.4",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "ng": "ng",
    "upgrade": "ng update --all",
    "build": "ng build --configuration dev && npm run build:widgets",
    "build-devstaging": "ng build --configuration devstaging && npm run build:widgets:devstaging",
    "build-devbeta": "ng build --configuration devbeta && npm run build:widgets:devbeta",
    "build-staging": "ng build --configuration staging && npm run build:widgets:staging",
    "build-beta": "ng build --configuration beta && npm run build:widgets:beta",
    "build-prod": "ng build --configuration prod && npm run build:widgets:prod",
    "build-prodai": "ng build --configuration prodai && npm run build:widgets:prodai",
    "build:widgets": "ng build package-chooser-widget --configuration=dev --extract-css false && node widgets-build.js",
    "build:bank-code": "ng build bank-code --configuration prod",
    "build:widgets:devstaging": "ng build package-chooser-widget --configuration devstaging --extract-css false && node widgets-build.js",
    "build:widgets:devbeta": "ng build package-chooser-widget --configuration devbeta --extract-css false && node widgets-build.js",
    "build:widgets:staging": "ng build package-chooser-widget --configuration staging --extract-css false && node widgets-build.js",
    "build:widgets:beta": "ng build package-chooser-widget --configuration beta --extract-css false && node widgets-build.js",
    "build:widgets:prod": "ng build package-chooser-widget --configuration prod --extract-css false && node widgets-build.js",
    "start": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng serve --host 0.0.0.0 --port 7200 --configuration dev",
    "start-staging": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng serve --host 0.0.0.0 --poll 5000 --port 7200 --configuration devstaging",
    "start-beta": "ng serve --host 0.0.0.0 --poll 1000 --port 7200 --configuration devbeta",
    "start-beta-ssl": "ng serve --host 0.0.0.0 --port 7200 --configuration devbeta --ssl --ssl-cert ssl/server.crt --ssl-key ssl/server.key",
    "start-staging-ssl": "ng serve --host 0.0.0.0 --port 7200 --configuration devstaging --ssl --ssl-cert ssl/server.crt --ssl-key ssl/server.key",
    "debug": "ng serve --host 0.0.0.0 --port 7200 --configuration=dev --source-map true",
    "debug-ossllp": "cross-env NODE_OPTIONS=\"--openssl-legacy-provider --max-old-space-size=8192\" ng serve --host 0.0.0.0 --port 7200 --configuration=dev --source-map true",
    "debug-staging": "ng serve --host 0.0.0.0 --port 7200 --configuration devstaging --source-map true",
    "debug-staging-ossllp": "cross-env NODE_OPTIONS=\"--openssl-legacy-provider --max-old-space-size=8192\" ng serve --host 0.0.0.0 --port 7200 --configuration devstaging --source-map true",
    "test": "ng test",
    "pree2e": "webdriver-manager update --standalone false --gecko false",
    "e2e": "protractor",
    "bundle-report": "node --max_old_space_size=8192 node_modules/@angular/cli/bin/ng build --configuration=prod --stats-json && npm run stats",
    "stats": "webpack-bundle-analyzer dist/stats.json --port 9200",
    "fix-memory-limit": "cross-env LIMIT=8192 increase-memory-limit",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points"
  },
